@model List<ApplicantSite.Models.ApplicantModel>
@{
    ViewBag.Title = "Applicant Information";
}

<h2>Applicant Information</h2>

@{Html.RenderPartial("Messages"); }
@Html.ActionLink("Create New", "Create")
1
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model[0].FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].ZipCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].Phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].SalaryRequired)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].Level_of_Expertise)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].YearsOfExperience)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].EducationLevel)
        </th>
    </tr>
    <tbody>
        @foreach (var item in Model)
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ZipCode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Phone)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SalaryRequired)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Level_of_Expertise)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.YearsOfExperience)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EducationLevel)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id })
                </td>
            </tr>
        }
    </tbody>
</table>
